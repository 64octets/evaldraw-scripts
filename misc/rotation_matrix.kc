()
{
   time = klock(0);
   glEnable(GL_DEPTH_TEST);
   cls(0); 
   clz(1e32);// corresponds to glClear( GL_DEPTH_BUFFER_BIT );
   
  

   setcam(0,0,-5, 0,0);
   
   //static mat[9];
   
ang = mousx/xres * 2 *PI;

scale = 1 + sin( time*10 )/16;

rx = -1 * scale; 
ry = 0;
rz = 0;

dx= 0;
dy= cos(ang) * scale;
dz= -sin(ang) * scale;

fx=0;
fy= sin(ang) * scale;
fz= cos(ang) * scale;

x=0;
y=0;
z=0;
   
   drawspr("cow.kv6",x,y,z,rx,ry,rz,dx,dy,dz,fx,fy,fz);
}